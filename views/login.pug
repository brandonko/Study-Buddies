extends layout

block content
    .limiter
      .container-login100
        .wrap-login100.p-t-50.p-b-90
          form.login100-form.validate-form.flex-sb.flex-w
            span.login100-form-title.p-b-51
              | Login into Study Buddies
            .wrap-input100.validate-input.m-b-16(data-validate='Username is required')
              input.input100(type='text', name='username', placeholder='Username')
              span.focus-input100
            .wrap-input100.validate-input.m-b-16(data-validate='Password is required')
              input.input100(type='password', name='pass', placeholder='Password')
              span.focus-input100
            .container-login100-form-btn.m-t-17
              button.login100-form-btn
                | Login
    #dropDownSelect1
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js', charset='utf-8', type='text/javascript')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js', integrity='sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q', crossorigin='anonymous')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js', integrity='sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl', crossorigin='anonymous')
